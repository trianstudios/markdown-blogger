{
  "name": "trianstudios/press",
  "description": "An elegant markdown-powered blog for the laravel framework",
  "keywords": [
    "framework",
    "laravel",
    "package",
    "blogger",
    "markdown"
  ],
  "homepage": "https://github.com/trianstudios/markdown-blogger",
  "license": "MIT",
  "authors": [
    {
      "homepage": "https://trianstudios.com",
      "name": "Trian Studios"
    }
  ],
  "require-dev": {
    "orchestra/testbench": "^6.23",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "trianstudios\\Press\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "trianstudios\\Press\\Tests\\": "tests/",
      "trianstudios\\Press\\Database\\Factories\\": "database/factories/"
    }
  },
  "require": {
    "php": ">=7.4",
    "erusev/parsedown": "^1.7",
    "ext-json": "*",
    "laravel/legacy-factories": "^1.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "trianstudios\\Press\\PressBaseServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
